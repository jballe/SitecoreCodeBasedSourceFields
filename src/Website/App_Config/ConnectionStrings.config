  <add name="master"    connectionString="Server=(LocalDB)\MSSQLLocalDB; Integrated Security=true;AttachDbFileName=|DataDirectory|\Databases\Sitecore.Master.mdf" />
  <add name="web"       connectionString="Server=(LocalDB)\MSSQLLocalDB; Integrated Security=true;AttachDbFileName=|DataDirectory|\Databases\Sitecore.Web.mdf" />
  <add name="analytics" connectionString="mongodb://localhost/analytics" />
  <add name="tracking.live" connectionString="mongodb://localhost/tracking_live" />
  <add name="tracking.history" connectionString="mongodb://localhost/tracking_history" />
  <add name="tracking.contact" connectionString="mongodb://localhost/tracking_contact" />
  <add name="reporting" connectionString="Server=(LocalDB)\MSSQLLocalDB; Integrated Security=true;AttachDbFileName=|DataDirectory|\Databases\Sitecore.Analytics.mdf" />
